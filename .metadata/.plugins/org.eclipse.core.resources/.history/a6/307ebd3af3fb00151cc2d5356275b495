var express = require("express");
var app = express();
    app.set("view engine","jade")

/*
 * 
app.get("/",function(req,res){
	res.render("home.jade")
})
 * 
 */
app.get("/",function(req,res){
	res.render("home.ejs",{
		msg:"welcome to ejs",
		title:"welcome"
	})
})


app.listen(5678);
console.log("serve ris running on localhost:5678");

