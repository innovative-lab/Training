(function(){
	angular
	   .module("app")
	   .config(function($routeProvider){
	   	$routeProvider
		     	     .when("/",{
		     	     	controller:"logincontroller",
		     	     	templateUrl:"app/view/loginPage.html"
		     	     })
		     	     .when("/menu",{
		     	     	controller:"menucontroller",
		     	     	templateUrl:"app/views/mainPage.html"
		     	     })
		     	     .when("/currentevent",{
		     	     	controller:"currentEventcontroller",
		     	     	templateUrl:"app/views/eventDetailsPage.html"
		     	     })
		     	     .when("/createevent",{
		     	     	controller:"createEventcontroller",
		     	     	templateUrl:"app/views/createEventPage.html"
		     	     })
		     	     
	   })
}())

