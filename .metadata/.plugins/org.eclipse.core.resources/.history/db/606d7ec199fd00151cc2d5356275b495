    	}
	  	 
	  	 
	  	$scope.callToAddToDetails = function(currObj){
        	loginservice.add_details(currObj);
    	};
    	
    	$scope.callToAddToDetails(data);
    	
    
    
    
	  	 //alert($rootScope.mid1)
	  	
