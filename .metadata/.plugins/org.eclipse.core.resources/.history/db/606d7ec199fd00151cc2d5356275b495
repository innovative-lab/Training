(function(){
	  
	angular
	  .module("app")
	  .controller("logincontroller",function($scope,$rootScope,loginservice){
	  	
	  	 // $rootScope.u_mid=mid1
	  	 // alert($root.u_mid);
    	$scope.data={
    		mid:$scope.mid1,
